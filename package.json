{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "dotenv -e .env -- next build",
    "dev": "dotenv -e .env -- next dev",
    "lint": "dotenv -e .env -- next lint",
    "start": "dotenv -e .env -- next start",
    "tf:apply": "cd terraform && dotenv -e ../.env -- terraform apply",
    "tf:destroy": "cd terraform && dotenv -e ../.env -- terraform destroy",
    "tf:init": "cd terraform && dotenv -e ../.env -- terraform init",
    "tf:plan": "cd terraform && dotenv -e ../.env -- terraform plan"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.3.1",
    "next": "13.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "@types/eslint": "^8.21.0",
    "@types/node": "20.1.2",
    "@types/prettier": "^2.7.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "10.4.14",
    "dotenv-cli": "^7.0.0",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-turbo": "^0.0.9",
    "eslint-plugin-react": "7.32.2",
    "postcss": "8.4.23",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "packageManager": "npm@9.5.0",
  "engines": {
    "node": ">=16.0.0"
  }
}
